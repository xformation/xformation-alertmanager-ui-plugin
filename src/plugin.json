{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Alert manager",
  "id": "xformation-alertmanager-ui-plugin",
  "info": {
    "description": "Alert manager",
    "author": {
      "name": "Synectiks"
    },
    "logos": {
      "small": "img/asset_logo.svg",
      "large": "img/asset_logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "MonitorAlerts",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%"
    },
    {
      "type": "page",
      "name": "AllAlerts",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=all-alerts"
    },
    {
      "type": "page",
      "name": "Rules",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=rules"
    },
    {
      "type": "page",
      "name": "CreateRule",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=create-rule"
    },
    {
      "type": "page",
      "name": "AllTickets",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=all-tickets"
    },
    {
      "type": "page",
      "name": "AlertRuleBuilder",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=alert-rule-builder"
    },
    {
      "type": "page",
      "name": "ManageAlertRule",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=manage-alert-rule"
    },
    {
      "type": "page",
      "name": "ScriptEditor",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=script-editor"
    },
    {
      "type": "page",
      "name": "SearchAlert",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=search-alert"
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=8.0.0",
    "plugins": []
  }
}